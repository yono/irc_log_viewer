<h1>Listing logs</h1>

<table>
  <tr>
    <th>Log</th>
    <th>Channel</th>
    <th>Nick</th>
    <th>Is privmsg</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.log %></td>
    <td><%= log.channel_id %></td>
    <td><%= log.nick_id %></td>
    <td><%= log.is_privmsg %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
